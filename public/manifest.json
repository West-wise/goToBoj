{
  "name": "백준 문제 바로가기",
  "version": "1.0",
  "manifest_version": 3,
  "description": "백준 온라인 저지 문제 번호로 바로 이동하는 확장프로그램",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
